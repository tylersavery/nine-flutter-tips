    "version": "0.2.0",
    "configurations": [
        {
            "name": "Develop",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "FLAVOR=develop"
            ]
        },
        {
            "name": "Prod",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "FLAVOR=prod"
            ]
        },
        {
            "name": "Prod (Web)",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "42069",
                "--dart-define",
                "FLAVOR=prod"
            ]
        },
    ]
}